        xmlns:ll="clr-namespace:LinePutScript.Localization.WPF;assembly=LinePutScript.Localization.WPF"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        mc:Ignorable="d" xmlns:pu="clr-namespace:Panuon.WPF.UI;assembly=Panuon.WPF.UI" Title="{ll:Str MoreTTS 设置}" Width="600"
        FontSize="16" Height="Auto" ResizeMode="NoResize" WindowState="Normal" SizeToContent="Height"
        Background="{DynamicResource PrimaryLighter}" Closed="Window_Closed">
    <Window.Resources>
        <ResourceDictionary Source="/VPet-Simulator.Windows.Interface;component/ResourceStyle.xaml" />
    </Window.Resources>
    <Grid Margin="8" Background="{DynamicResource PrimaryLighter}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="15" />
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="2*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="{ll:Str 启用}" VerticalAlignment="Center" />
        <TextBlock Grid.Row="1" Text="{ll:Str 音量}" VerticalAlignment="Center" />
        <TextBlock Grid.Row="2" Text="{ll:Str 语言}" VerticalAlignment="Center" />
        <TextBlock Grid.Row="3" Text="{ll:Str 讲述人}" VerticalAlignment="Center" />
        <TextBlock Grid.Row="4" Text="{ll:Str 'noise_scale(感情变化程度)'}" VerticalAlignment="Center" />
        <TextBlock Grid.Row="5" Text="{ll:Str 'noise_scale_w(控制音素发音长度)'}" VerticalAlignment="Center" />
        <TextBlock Grid.Row="6" Text="{ll:Str 'length_scale(控制整体语速)'}" VerticalAlignment="Center" />

        <pu:Switch x:Name="SwitchOn" BorderBrush="{DynamicResource DARKPrimaryDark}"
                CheckedBackground="{DynamicResource DARKPrimary}" CheckedBorderBrush="{DynamicResource DARKPrimary}"
                Background="Transparent" BoxWidth="35" BoxHeight="18" ToggleSize="14" ToggleShadowColor="{x:Null}"
                ToggleBrush="{DynamicResource DARKPrimaryDark}" Content="{ll:Str 启用 MoreTTS}"
                CheckedToggleBrush="{DynamicResource DARKPrimaryText}" Grid.Column="2" />

        <pu:NumberInput Grid.Row="1" Grid.Column="3" Minimum="0" MaxHeight="350" Margin="5"
                Value="{Binding Value, ElementName=VolumeSilder}" CornerRadius="3" Maximum="100" />
        <Slider Grid.Row="1" Grid.Column="2" x:Name="VolumeSilder" Maximum="100" Value="100" VerticalAlignment="Center"
                Background="{DynamicResource DARKPrimaryLighter}" LargeChange="10" SmallChange="2"
                pu:SliderHelper.CoveredBackground="{DynamicResource DARKPrimaryDarker}" />

        <pu:NumberInput Grid.Row="4" Grid.Column="3" Minimum="0.1" Maximum="1.0" MaxHeight="350" Margin="5"
                Value="{Binding Value, ElementName=NoiseScaleSlider}" CornerRadius="3"/>
        <Slider Grid.Row="4" Grid.Column="2" x:Name="NoiseScaleSlider" Minimum="0.1" Maximum="1.0" Value="0.6" VerticalAlignment="Center"
                Background="{DynamicResource DARKPrimaryLighter}" SmallChange="0.1"
                pu:SliderHelper.CoveredBackground="{DynamicResource DARKPrimaryDarker}" />

        <pu:NumberInput Grid.Row="5" Grid.Column="3" Minimum="0.1" Maximum="1.0" MaxHeight="350" Margin="5"
                Value="{Binding Value, ElementName=NoiseScaleWSlider}" CornerRadius="3"/>
        <Slider Grid.Row="5" Grid.Column="2" x:Name="NoiseScaleWSlider" Minimum="0.1" Maximum="1.0" Value="0.668" VerticalAlignment="Center"
                Background="{DynamicResource DARKPrimaryLighter}" SmallChange="0.1"
                pu:SliderHelper.CoveredBackground="{DynamicResource DARKPrimaryDarker}" />

        <pu:NumberInput Grid.Row="6" Grid.Column="3" Minimum="0.1" Maximum="2.0" MaxHeight="350" Margin="5"
                Value="{Binding Value, ElementName=LengthScaleSilder}" CornerRadius="3"/>
        <Slider Grid.Row="6" Grid.Column="2" x:Name="LengthScaleSilder" Minimum="0.1" Maximum="2.0" Value="1.1" VerticalAlignment="Center"
                Background="{DynamicResource DARKPrimaryLighter}" SmallChange="0.1"
                pu:SliderHelper.CoveredBackground="{DynamicResource DARKPrimaryDarker}" />

        <ComboBox x:Name="Language" Grid.Row="2" Grid.Column="2" Style="{DynamicResource StandardComboBoxStyle}"
